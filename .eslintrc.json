{
  "env": {
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [{
    "files": ["test/**/*.js"],
    "env": {
      "mocha": true
    },
    "rules": {
      "func-names": [0],
      "import/no-extraneous-dependencies": [0],
      "prefer-arrow-callback": [0],
      "arrow-body-style": [0],
      "no-unused-expressions": [0]
    }
  }]
}
